  "scripts": {
    "dev": "NODE_ENV=development REACT_APP_DEMO_MODE=true REACT_APP_BYPASS_AUTH=true vite --open",
    "dev:host": "NODE_ENV=development REACT_APP_DEMO_MODE=true REACT_APP_BYPASS_AUTH=true vite --host --open",
    "build": "tsc && vite build",
    "build:no-typecheck": "vite build",
    "build:check": "tsc && vite build --mode development",
    "preview": "vite preview --open",
    "start": "NODE_ENV=development REACT_APP_DEMO_MODE=true REACT_APP_BYPASS_AUTH=true vite --open",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "check": "npm run typecheck && npm run lint && npm run format:check",
    "check:fix": "npm run typecheck && npm run lint:fix && npm run format",
    "clean": "rm -rf node_modules package-lock.json build dist && npm install",
    "clean:cache": "rm -rf node_modules/.vite && rm -rf node_modules/.cache",
    "analyze": "npm run build && npx vite-bundle-analyzer",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "prepare": "husky install",
    "start:no-lint": "NODE_ENV=development REACT_APP_DEMO_MODE=true REACT_APP_BYPASS_AUTH=true vite --open --config vite.config.mts",
    "emergency-install": "rm -rf node_modules package-lock.json && npm install --legacy-peer-deps",
    "test:business-lookup": "vitest run --testNamePattern=BusinessLookup",
    "debug:business-lookup": "REACT_APP_DEBUG_BUSINESS_LOOKUP=true npm start",
    "test:filelock": "jest src/components/document/__tests__/FileLockImmutableLedger.test.tsx",
    "test:filelock:integration": "jest src/services/__tests__/unifiedStorageService.test.ts",
    "test:filelock:e2e": "cypress run --spec cypress/e2e/filelock-immutable-ledger.cy.ts",
    "test:filelock:qa": "ts-node src/qa/runQATests.ts",
    "test:filelock:all": "npm run test:filelock && npm run test:filelock:integration && npm run test:filelock:e2e",
    "type-check": "tsc --noEmit",
    "test:a11y": "vitest run --testNamePattern=a11y",
    "build:staging": "NODE_ENV=staging npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer",
    "deploy:staging": "npm run build:staging && wrangler pages deploy build --project-name evafi-ai-fe-demo --branch staging",
    "deploy:production": "npm run build:production && wrangler pages deploy build --project-name evafi-ai-fe-demo --branch main",
    "security:audit": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "ci:install": "npm ci --legacy-peer-deps",
    "ci:test": "npm run type-check && npm run lint && npm run test:run",
    "ci:build": "npm run build",
    "ci:deploy": "npm run deploy:staging",
    "setup:branch-protection": "./scripts/setup-branch-protection.sh",
    "compliance:check": "./scripts/compliance-automation.sh",
    "compliance:report": "./scripts/compliance-automation.sh && open compliance-reports/",
    "quality:gates": "npm run lint && npm run type-check && npm run test:run && npm run compliance:check",
    "security:scan": "npm audit --audit-level=moderate && npm run security:dependencies",
    "security:dependencies": "npx audit-ci --moderate || echo 'audit-ci not installed'",
    "test:financial": "npm run test:run -- --testNamePattern='financial|calculation|credit|risk'",
    "test:security": "npm run test:run -- --testNamePattern='auth|security|encrypt|permission'",
    "test:compliance": "npm run test:financial && npm run test:security && npm run test:a11y",
    "code-review:prepare": "npm run quality:gates && echo 'Code review preparation complete'",
    "soc2:validate": "./scripts/compliance-automation.sh --soc2-only",
    "organizational:check": "./scripts/compliance-automation.sh --org-rules-only"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "error",
      "react-hooks/rules-of-hooks": "error",
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
