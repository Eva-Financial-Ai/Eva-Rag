# Cloudflare Configuration
VITE_CLOUDFLARE_ACCOUNT_ID=your-cloudflare-account-id
# Staging Domain (evafin.ai)
VITE_CLOUDFLARE_ZONE_ID_STAGING=your-staging-zone-id
# Production Domain (evafi.ai)
VITE_CLOUDFLARE_ZONE_ID_PRODUCTION=your-production-zone-id
VITE_CLOUDFLARE_API_TOKEN=your-cloudflare-api-token
VITE_CLOUDFLARE_EMAIL=your-cloudflare-email
VITE_R2_STORAGE_BUCKET=your-r2-storage-bucket-url
VITE_CLOUDFLARE_IMAGES_API=your-cloudflare-images-api-key
VITE_CLOUDFLARE_STREAM_SUBDOMAIN=your-cloudflare-stream-subdomain

# Environment Configuration
VITE_ENVIRONMENT=staging
VITE_API_URL=https://api.evafin.ai

# Auth0 Configuration
VITE_AUTH0_DOMAIN=your-auth0-domain
VITE_AUTH0_CLIENT_ID=your-auth0-client-id
VITE_AUTH0_AUDIENCE=your-auth0-audience
VITE_AUTH0_LOGIN_URL=your-application-login-url

# Financial Services
VITE_PLAID_CLIENT_ID=your-plaid-client-id
VITE_PLAID_PUBLIC_KEY=your-plaid-public-key
VITE_PLAID_VERIFICATION_LINK=your-plaid-verification-link
VITE_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key

# Additional Services
VITE_GEOAPIFY_API_KEY=your-geoapify-api-key

# Notification Service (Twilio SendGrid)
VITE_TWILIO_SENDGRID_NAME=your-twilio-sendgrid-name
VITE_TWILIO_SID=your-twilio-sid
VITE_TWILIO_SECRET=your-twilio-secret
VITE_TWILIO_PRIMARY_TOKEN=your-twilio-primary-token
VITE_TWILIO_TEST_AUTH_TOKEN=your-twilio-test-auth-token
VITE_TWILIO_PHONE_NUMBER=your-twilio-phone-number
VITE_TWILIO_SMS_NOTIFICATION=your-twilio-sms-notification-number

# Supabase Configuration
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_KEY=your-supabase-service-role-key

# Database Connection
VITE_DATABASE_URL=your-database-connection-string

# Demo Mode
VITE_DEMO_MODE=false

# For backward compatibility with REACT_APP_ prefix
REACT_APP_ENVIRONMENT=staging
REACT_APP_API_URL=https://api.evafin.ai
REACT_APP_AUTH0_DOMAIN=your-auth0-domain
REACT_APP_AUTH0_CLIENT_ID=your-auth0-client-id
REACT_APP_AUTH0_AUDIENCE=your-auth0-audience
REACT_APP_PLAID_CLIENT_ID=your-plaid-client-id
REACT_APP_PLAID_PUBLIC_KEY=your-plaid-public-key
REACT_APP_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key
REACT_APP_GEOAPIFY_API_KEY=your-geoapify-api-key